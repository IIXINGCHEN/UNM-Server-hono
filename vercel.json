{
  "version": 2,
  "builds": [
    {
      "src": "./dist/app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/views/**",
          "src/public/**"
         ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/app.js"
    }
  ]
}
